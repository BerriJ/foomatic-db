<printer id="printer/25216">
  <make>Okidata</make>
  <model>Okipage 4w+</model>
  <mechanism>
    <led/><!--not "color"-->
    <resolution>
      <dpi>
        <x>300</x>
        <y>300</y>
      </dpi>
    </resolution>
    <consumables>
      <comments>
        <en>replacement only 5210/6201 $49.00Cdn</en>
      </comments>
    <!--one or more "partno" elements.--></consumables>
  </mechanism><!--no "url"-->
  <lang>
    <proprietary/><!--no pjl-->
    <text>
      <charset>us-ascii</charset>
    </text>
  </lang>
  <autodetect>
    <parallel>
      <commandset>HIPERWINDOWS,OPEL</commandset>
      <description>OKI</description>
      <manufacturer>OKI</manufacturer>
      <model>OKI</model>
    </parallel>
  </autodetect>
  <functionality>B</functionality>
  <driver>oki4w</driver>
  <unverified/><!--no "contrib_url"-->
  <comments>
    <en>
    The oki4linux driver package drives this printer properly, but
    it&apos;s a bit klunky to use.
    </en>
  </comments>
</printer>
