  <lang>
    <pcl level="5e"/><!--no pjl-->
    <text>
      <charset>us-ascii</charset>
    </text>
  </lang>
  <autodetect>
  <!--no known parport probe information--></autodetect>
  <functionality>B</functionality>
  <driver>hl1250</driver><!--verified--><!--no "contrib_url"-->
