<driver id="driver/lj4dithp">
  <name>lj4dithp</name>
  <url>http://www.linuxprinting.org/download/japanese_printers/</url>
  <execution>
    <ghostscript/>
    <prototype>gs -q -dBATCH -dSAFER -dQUIET -dNOPAUSE -sDEVICE=lj4dithp%A%Z -sOutputFile=- - | perl -e &quot;while (\$line = &lt;&gt;) { \$match |= ( \$line =~ s/\x1b&amp;l\d+A/\$&amp;%E/ ); print \$line }&quot;</prototype>
    </execution>
  <comments>
    <en>
      Special japanese printer driver.
      </en>
    </comments>
  <printers>
    <printer>
      <id>printer/HP-LaserJet_4V_4LJ_Pro</id>
      </printer>
    </printers>
  </driver>
