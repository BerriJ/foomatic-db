<driver id="driver/escpage">
  <name>escpage</name>
  <url>http://www.linuxprinting.org/download/japanese_printers/</url>
  <execution>
    <ghostscript/>
    <prototype>gs -q -dBATCH -dSAFER -dQUIET -dNOPAUSE -sDEVICE=escpage%A%Z -sOutputFile=- -</prototype>
    </execution>
  <comments>
    <en>
      Special japanese printer driver.
      </en>
    </comments>
  <printers>
    <printer>
      <id>printer/Epson-LP-xx00</id>
      </printer>
    </printers>
  </driver>
