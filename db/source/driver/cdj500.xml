<driver id="driver/cdj500">
 <name>cdj500</name>
 <url>http://www.ghostscript.com/doc/gnu/7.05/Devices.htm#HP_color_inkjet</url>
 <execution>
  <ghostscript />
  <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -r300x300 -sDEVICE=cdj500%A%Z -sOutputFile=- -</prototype>
 </execution>
 
