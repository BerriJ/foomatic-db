<driver id="driver/ljetplus">
 <name>ljetplus</name>
 <url>http://www.cs.wisc.edu/~ghost/doc/printer.htm</url>
 <execution>
  <ghostscript />
  <prototype>gs -q -dBATCH -dSAFER -dQUIET -dNOPAUSE -sDEVICE=ljetplus%A%Z -sOutputFile=- - | perl -e &quot;while (\$line = &lt;&gt;) { \$match |= ( \$line =~ s/\x1b&amp;l\d+A/\$&amp;%E/ ); print \$line }&quot;</prototype>
 </execution>
 
 <printers>
  <printer>
   <id>printer/74528</id><!-- Brother HL-8 -->
  </printer>
  <printer>
   <id>printer/23040</id><!-- HP LaserJet Plus -->
  </printer>
  <printer>
   <id>printer/158816</id><!-- Kyocera F-800T -->
  </printer>
  <printer>
   <id>printer/70528</id><!-- Kyocera F-3300 -->
  </printer>
  <printer>
   <id>printer/70496</id><!-- Kyocera P-2000 -->
  </printer>
  <printer>
   <id>printer/126496</id><!-- Lexmark Winwriter 400 -->
  </printer>
 </printers>
</driver>