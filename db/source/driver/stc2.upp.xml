<driver id="driver/stc2.upp">
 <name>stc2.upp</name>
 <url>http://www.ghostscript.com/doc/gnu/7.05/Devices.htm#Uniprint</url>
 <execution>
  <uniprint />
  <prototype>gs %B -q -dBATCH -dSAFER -dQUIET -dNOPAUSE %A%Z -sOutputFile=- -</prototype>
 </execution>
 <comments>
  <en>The printing system/execution information for this driver serves for all the
stc2*.upp drivers.</en>
 </comments>
 <printers>
  <printer>
   <id>printer/80896</id><!-- Epson Stylus Color 860 -->
  </printer>
  <printer>
   <id>printer/61216</id><!-- Epson Stylus Color II -->
  </printer>
  <printer>
   <id>printer/61248</id><!-- Epson Stylus Color IIs -->
  </printer>
 </printers>
</driver>