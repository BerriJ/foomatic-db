<driver id="driver/md50Mono">
  <name>md50Mono</name>
  <url>http://www.linuxprinting.org/download/japanese_printers/</url>
  <execution>
    <ghostscript/>
    <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=md50Mono%A%Z -sOutputFile=- -</prototype>
    </execution>
  <comments>
    <en>
      Special japanese printer driver.
      </en>
    </comments>
  <printers>
    <printer>
      <id>printer/240041</id> <!-- Alps-MD-5000 -->
      </printer>
    <printer>
      <id>printer/599218</id> <!-- Okidata DP 5000 -->
      </printer>
    </printers>
  </driver>
