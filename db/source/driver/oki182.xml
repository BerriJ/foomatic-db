<driver id="driver/oki182">
 <name>oki182</name>
 <url>http://www.cs.wisc.edu/~ghost/doc/printer.htm</url>
 <execution>
  <ghostscript />
  <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=oki182%A%Z -sOutputFile=- -</prototype>
 </execution>
 
 <printers>
  <printer>
   <id>printer/Oki-Microline_182</id><!-- Oki Microline 182 -->
  </printer>
  <printer>
   <id>printer/Oki-Microline_192plus</id><!-- Oki Microline 192+ -->
  </printer>
 </printers>
</driver>