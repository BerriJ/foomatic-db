<driver id="driver/eps9mid">
 <name>eps9mid</name>
 <url>http://www.cs.wisc.edu/~ghost/doc/printer.htm</url>
 <execution>
  <ghostscript />
  <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=eps9mid%A%Z -sOutputFile=- -</prototype>
 </execution>
 
 <printers>
  <printer>
   <id>printer/Generic-ESC_P_Dot_Matrix_Printer</id>
  </printer>
  <printer>
   <id>printer/24448</id><!-- Epson Dot Matrix -->
  </printer>
  <printer>
   <id>printer/125632</id><!-- QMS LPK-100 -->
  </printer>
  <printer>
   <id>printer/475634</id><!-- Star NX-1001 -->
  </printer>
 </printers>
</driver>