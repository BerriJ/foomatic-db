<driver id="driver/epsonc">
 <name>epsonc</name>
 <url>http://www.cs.wisc.edu/~ghost/doc/printer.htm</url>
 <execution>
  <ghostscript />
  <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=epsonc%A%Z -sOutputFile=- -</prototype>
 </execution>
 
 <printers>
  <printer>
   <id>printer/73760</id><!-- Brother MFC 7150C -->
  </printer>
  <printer>
   <id>printer/83072</id><!-- Brother MFC-9100c -->
   <comments>
    <en>Won't do color. Some resolutions don't keep proper aspect.</en>
   </comments>
  </printer>
  <printer>
   <id>printer/24448</id><!-- Epson Dot Matrix -->
  </printer>
  <printer>
   <id>printer/24320</id><!-- Epson LQ-24 -->
  </printer>
  <printer>
   <id>printer/23904</id><!-- Epson LQ-2550 -->
  </printer>
  <printer>
   <id>printer/24128</id><!-- Fujitsu 1200 -->
  </printer>
  <printer>
   <id>printer/24192</id><!-- Fujitsu 2400 -->
  </printer>
  <printer>
   <id>printer/24256</id><!-- Fujitsu 3400 -->
  </printer>
  <printer>
   <id>printer/138697</id><!-- Panasonic KX-P2135 -->
  </printer>
  <printer>
   <id>printer/80288</id><!-- Star LC24-200 -->
   <comments>
    <en>Probably this is the correct driver for colour printing.</en>
   </comments>
  </printer>
  <printer>
   <id>printer/24768</id><!-- NEC PinWriter P6 -->
  </printer>
  <printer>
   <id>printer/24832</id><!-- NEC PinWriter P6 plus -->
  </printer>
 </printers>
</driver>