<driver id="driver/ibmpro">
 <name>ibmpro</name>
 <url>http://www.cs.wisc.edu/~ghost/doc/printer.htm</url>
 <execution>
  <ghostscript />
  <prototype>gs -q -dBATCH -dSAFER -dQUIET -dNOPAUSE -sDEVICE=ibmpro%A%Z -sOutputFile=- -</prototype>
 </execution>
 
 <printers>
  <printer>
   <id>printer/204745</id><!-- IBM ProPrinterII -->
  </printer>
  <printer>
   <id>printer/75936</id><!-- Okidata Microline 192+ -->
  </printer>
  <printer>
   <id>printer/70208</id><!-- Panasonic KX-P2150 -->
  </printer>
 </printers>
</driver>