<driver id="driver/lbp310">
  <name>lbp310</name>
  <url>http://www.linuxprinting.org/download/japanese_printers/</url>
  <execution>
    <ghostscript/>
    <prototype>gs -q -dBATCH -dSAFER -dQUIET -dNOPAUSE -sDEVICE=lbp310%A%Z -sOutputFile=- -</prototype>
    </execution>
  <comments>
    <en>
      Special japanese printer driver.
      </en>
    </comments>
  <printers>
    <printer>
      <id>printer/Canon-LBP-310</id>
      </printer>
    </printers>
  </driver>
