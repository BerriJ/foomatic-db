<driver id="driver/stc1520h.upp">
  <name>stc1520h.upp</name>
  <url>http://www.ghostscript.com/</url>
  <obsolete replace="gutenprint" />
  <thirdpartysupplied />
  <execution>
    <uniprint />
    <prototype>gs @stc1520h.upp -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE %A%Z -sOutputFile=- -</prototype>
  </execution>
  <printers>
    <printer>
      <id>printer/Epson-Stylus_Color_1520</id>
    </printer>
    <printer>
      <id>printer/Epson-Stylus_Color_800</id>
      <comments>
        <en>1440x720dpi 32-bit CMYK, 62-pin, inkjet paper</en>
      </comments>
    </printer>
  </printers>
</driver>
